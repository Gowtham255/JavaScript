var s1="Hello World";
var s3=s1.split("");
const s2 = s3.filter(element => element.trim());
var uppercount=0,lowercount=0;
for(i=0;i<s2.length;i++){
  if(s2[i]==s2[i].toUpperCase()){
    uppercount=uppercount+1;
  }
  if(s2[i]==s2[i].toLowerCase()){
    lowercount=lowercount+1;
  }
}
console.log("uppercase count:"+uppercount);
console.log("lowercase count:"+lowercount)
